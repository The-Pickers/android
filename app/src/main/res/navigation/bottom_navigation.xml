<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="gsc.ZupStar.sampledata.HomeFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_home"
        />
    <fragment
        android:id="@+id/navigation_map"
        android:name="gsc.ZupStar.sampledata.MapFragment"
        android:label="@string/nav_map"
        tools:layout="@layout/fragment_map" />
    <fragment
        android:id="@+id/navigation_ranking"
        android:name="gsc.ZupStar.sampledata.RankingFragment"
        android:label="@string/nav_ranking"
        tools:layout="@layout/fragment_ranking" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="gsc.ZupStar.sampledata.ProfileFragment"
        android:label="@string/nav_profile"
        tools:layout="@layout/fragment_profile" />
